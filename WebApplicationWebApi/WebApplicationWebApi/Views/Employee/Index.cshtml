@model IEnumerable<WebApplicationWebApi.Models.employee>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Salary)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>

}

</table> 
<input type="text" list="lists" id="txt" />
<datalist id="lists">
    @foreach (var item in Model)
    {
        <option value="@item.Id">@item.Name</option>
    }
</datalist>
<select>
    <option value="value">select name</option>
    @foreach (var item in Model)
    {
        <option  >@item.Name</option>
    }
</select>
<select>
    <option value="">select name</option>
    @foreach (var itema in ViewBag.employee )
    {
        <option value="@itema.Id">@itema.Name</option>
    }
</select>
@Html.DropDownList("Id", ViewBag.empl as SelectList ,"select employee",new {@class="form-control" })

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>
    $(function () {
        $("#txt").val('hide');
    })
</script>